<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://struts.apache.org/dtds/struts-config_1_2.dtd">
<struts-config>
	<form-beans>
	
		<form-bean name="DtjkFrom" type="com.jrsoft.fri.dtjk.from.DtjkFrom" />
   </form-beans>
		<action-mappings>
	  <!-- 电梯-->
	<action path="/elevatorAction" name="DtjkFrom"
			parameter="method" scope="request"
			type="org.springframework.web.struts.DelegatingActionProxy">
	</action>
	  <!-- 电梯白名单 -->
	 <action path="/phoneAction"  name="DtjkFrom"
			parameter="method" scope="request"
			type="org.springframework.web.struts.DelegatingActionProxy">
	</action>
	  <!-- 电梯网关-->
	<action path="/gatewayAction" name="DtjkFrom"
			parameter="method" scope="request"
			type="org.springframework.web.struts.DelegatingActionProxy">
	</action>
	
	<!-- 上报记录-->
	<action path="/recordAction" name="DtjkFrom"
			parameter="method" scope="request"
			type="org.springframework.web.struts.DelegatingActionProxy">
	</action>
	<!-- 电梯维保记录表-->
	<action path="/recordsAction"  name="DtjkFrom"
			parameter="method" scope="request"
			type="org.springframework.web.struts.DelegatingActionProxy">
	</action>
	 <!-- 电梯年检 -->
	 <action path="/inspectionAction"  name="DtjkFrom"
			parameter="method" scope="request"
			type="org.springframework.web.struts.DelegatingActionProxy">
	</action>
	
	

	</action-mappings>
</struts-config>
