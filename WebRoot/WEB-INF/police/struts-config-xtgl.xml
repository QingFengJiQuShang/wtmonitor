<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://struts.apache.org/dtds/struts-config_1_2.dtd">
<struts-config>
	<form-beans>
	
		<form-bean name="XtglForm" type="com.jrsoft.fri.xtgl.from.XtglForm" />
   </form-beans>
		<action-mappings>
	  <!-- 系统用户-->
	<action path="/usersAction" name="XtglForm"
			parameter="method" scope="request"
			type="org.springframework.web.struts.DelegatingActionProxy">
	</action>
	 
	   <!-- 维保单位-->
	<action path="/maintenanceUnitAction" name="XtglForm"
			parameter="method" scope="request"
			type="org.springframework.web.struts.DelegatingActionProxy">
	</action>
	
	 <!-- 维保人员-->
	<action path="/maintenanceUsersAction" name="XtglForm"
			parameter="method" scope="request"
			type="org.springframework.web.struts.DelegatingActionProxy">
	</action>
	
	 <!-- 使用单位-->
	<action path="/useUnitAction" name="XtglForm"
			parameter="method" scope="request"
			type="org.springframework.web.struts.DelegatingActionProxy">
	</action>
<!-- 救援单位-->
	<action path="/rescueUnitAction" name="XtglForm"
			parameter="method" scope="request"
			type="org.springframework.web.struts.DelegatingActionProxy">
	</action>
	<!-- 制造单位-->
	<action path="/makeUnitAction" name="XtglForm"
			parameter="method" scope="request"
			type="org.springframework.web.struts.DelegatingActionProxy">
	</action>
	<!-- 物业单位-->
	<action path="/propertyUnitAction" name="XtglForm"
			parameter="method" scope="request"
			type="org.springframework.web.struts.DelegatingActionProxy">
	</action>
	<!-- 区域单位-->
	<action path="/regionUnitAction" name="XtglForm"
			parameter="method" scope="request"
			type="org.springframework.web.struts.DelegatingActionProxy">
	</action>
	<!-- 保险单位-->
	<action path="/safeUnitAction" name="XtglForm"
			parameter="method" scope="request"
			type="org.springframework.web.struts.DelegatingActionProxy">
	</action>
	</action-mappings>
</struts-config>
